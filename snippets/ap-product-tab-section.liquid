<div class="container-fluid pt-5 pb-5">
            <div class="row d-flex justify-content-center">
                <div class="col-12 ap-panel-accord">
                    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                      {% assign count = 1 %}
                      {% for block in section.blocks %}
                      {% if block.type == "tab-section" %}
                        <div class="panel panel-default">
                            <div class="panel-heading" role="tab" id="heading{{count}}">
                                <h4 class="panel-title">
                                    <a role="button" data-toggle="collapse" data-parent="#accordion" data-target="#collapse-{{count}}" aria-expanded="false" aria-controls="collapse-{{count}}" class="">
                                        {{ block.settings.tab-hding }}
                                    </a>
                                </h4>
                            </div>
                            <div id="collapse-{{count}}" class="panel-collapse in collapse" role="tabpanel" aria-labelledby="heading-{{count}}" style="">
                                <div class="panel-body">
                                    {{ block.settings.tab-data }}
                                </div>
                            </div>
                        </div>
                      {% endif %}
                      {% assign count = count | plus: 1 %}
                      {% endfor %}
                    </div>
                </div>
            </div>
    </div>
